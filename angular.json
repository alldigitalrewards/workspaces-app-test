{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "top-level-app": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/top-level-app",
      "sourceRoot": "projects/top-level-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/top-level-app",
            "index": "projects/top-level-app/src/index.html",
            "browser": "projects/top-level-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/top-level-app/tsconfig.app.json",
            "assets": [
              "projects/top-level-app/src/favicon.ico",
              "projects/top-level-app/src/assets"
            ],
            "styles": [
              "projects/top-level-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "top-level-app:build:production"
            },
            "development": {
              "buildTarget": "top-level-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "top-level-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/top-level-app/tsconfig.spec.json",
            "assets": [
              "projects/top-level-app/src/favicon.ico",
              "projects/top-level-app/src/assets"
            ],
            "styles": [
              "projects/top-level-app/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
